{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "totem-webapp": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              { "glob": "**/*", "input": "src/assets/", "output": "/assets/" }
            ],
            "styles": [
              "node_modules/virtual-keyboard/dist/css/keyboard.min.css",
              "node_modules/animate.css/animate.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/libs/js/jquery-ui/jquery-ui.js",
              "src/libs/js/smoother.js",
              "src/libs/js/js-objectdetect/objectdetect.js",
              "src/libs/js/js-objectdetect/objectdetect.frontalface.js",
              "node_modules/virtual-keyboard/dist/js/jquery.keyboard.js",
              "node_modules/virtual-keyboard/dist/js/jquery.keyboard.extension-all.min.js"
            ]
          },
          "configurations": {
            "stage": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.stage.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "totem-webapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "totem-webapp:build:production"
            },
            "stage": {
              "browserTarget": "totem-webapp:build:stage"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "totem-webapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/libs/js/jquery-ui/jquery-ui.js",
              "src/libs/js/smoother.js",
              "src/libs/js/js-objectdetect/objectdetect.js",
              "src/libs/js/js-objectdetect/objectdetect.frontalface.js",
              "node_modules/virtual-keyboard/dist/js/jquery.keyboard.js",
              "node_modules/virtual-keyboard/dist/js/jquery.keyboard.extension-all.min.js",
              "node_modules/webrtc-adapter/out/adapter.js"
            ],
            "styles": [
              "node_modules/virtual-keyboard/dist/css/keyboard.min.css",
              "node_modules/animate.css/animate.css",
              "src/styles.scss"
            ],
            "assets": ["src/assets", "src/favicon.ico"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "totem-webapp-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "totem-webapp:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "totem-webapp",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
